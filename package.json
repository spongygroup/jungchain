{
  "name": "jung",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "sim": "SIM_MODE=true SIM_SPEED=60 tsx src/sim/sim-runner.ts",
    "chain:live": "npx tsx src/sim/live-relay.ts",
    "chain:photo": "npx tsx src/sim/live-relay-photo.ts",
    "sim:fast": "SIM_MODE=true SIM_SPEED=0 tsx src/sim/sim-runner.ts",
    "db:init": "tsx src/db/init.ts",
    "test": "vitest"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@google/generative-ai": "^0.24.1",
    "better-sqlite3": "^11.8.1",
    "dotenv": "^16.4.7",
    "geo-tz": "^8.1.5",
    "grammy": "^1.35.0",
    "luxon": "^3.5.0",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.10.5",
    "@types/node-cron": "^3.0.11",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
