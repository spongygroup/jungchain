    "dotenv": "^16.4.7",
    "geo-tz": "^8.1.5",
    "grammy": "^1.35.0",
    "luxon": "^3.5.0",
    "node-cron": "^3.0.3",
    "openai": "^6.22.0",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
