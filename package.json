    "@types/luxon": "^3.4.2",
    "@types/node": "^22.10.5",
    "@types/node-cron": "^3.0.11",
    "chai": "^6.2.2",
    "ethers": "^6.16.0",
    "hardhat": "^2.28.6",
    "hardhat-gas-reporter": "^2.3.0",
    "solidity-coverage": "^0.8.17",
    "tsx": "^4.19.2",
